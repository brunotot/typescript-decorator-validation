{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "declaration": true,
    "allowJs": true,
    "strict": true,
    "isolatedModules": false
  }
}
