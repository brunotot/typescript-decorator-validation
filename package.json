    "main": "npx ts-node --esm src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunotot/typescript-decorator-validation.git"
  },
  "keywords": [
    "typescript",
    "decorator",
    "form",
    "validation"
  ],
  "author": "Bruno Tot",
  "bugs": {
    "url": "https://github.com/brunotot/typescript-decorator-validation/issues"
  },
  "homepage": "https://github.com/brunotot/typescript-decorator-validation#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/preset-typescript": "^7.18.6",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
